{% extends 'base.html.twig' %}

{% block title %}Admin Order List{% endblock %}

{% block body %}

    <div class='container'>
        <div class="row">
            {{ form_errors(form) }}
        </div>
        <div class="row">
                <div class="card" >
                    <div class="card-header text-body">
                        <h5> Search Order Form</h5>
                        </div>
                    <div class="card-body row">
                        <div class="col-4">
                        {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}

                        {{ form_end(form) }}
                        </div>
                    </div>
            </div>
        </div>
        <hr>
    </div>

{% endblock %}
